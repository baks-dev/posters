{
  "name": "baks-dev/posters",
  "description": "Баннерная сеть",
  "homepage": "https://github.com/baks-dev/posters",
  "keywords": [
    "bundle",
    "symfony",
    "library",
    "reference",
    "banner",
    "poster",
    "baks"
  ],
  "license": "MIT",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Baks Development",
      "email": "admin@baks.dev"
    }
  ],
  "require": {
    "php": ">=8.4",
    "../core": "^7.2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "BaksDev\\Posters\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BaksDev\\Posters\\": "Tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true
    }
  }
}